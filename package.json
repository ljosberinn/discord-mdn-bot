{
  "name": "discord-mdn-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -r dotenv/config -x ts-node --files ./src/index.ts",
    "start": "node build/index.js",
    "build": "yarn test:ci && tsc",
    "lint": "eslint src && tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "lint:fix": "eslint --fix src && prettier --write src",
    "lint:types": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mdn/browser-compat-data": "^3.3.14",
    "@sentry/node": "6.11.0",
    "compare-versions": "3.6.0",
    "date-fns": "2.23.0",
    "discord.js": "^13.1.0",
    "dom-parser": "0.1.6",
    "domyno": "^1.0.1",
    "html-entities": "2.3.2",
    "lodash": "^4.17.21",
    "mongoose": "5.13.7",
    "node-cache": "5.1.2",
    "node-fetch": "2.6.1",
    "ts-node": "^10.2.0",
    "typescript": "4.3.5"
  },
  "devDependencies": {
    "@sentry/types": "6.11.0",
    "@types/dom-parser": "^0.1.1",
    "@types/html-entities": "^1.3.3",
    "@types/jest": "27.0.1",
    "@types/mongoose": "5.11.96",
    "@types/node": "16.6.1",
    "@types/node-fetch": "2.5.12",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-galex": "2.16.7",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lint-staged": "11.1.2",
    "nodemon": "2.0.12",
    "prettier": "2.3.2",
    "ts-jest": "27.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write",
    "*.js": "eslint --fix"
  }
}
