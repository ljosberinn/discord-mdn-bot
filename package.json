{
  "name": "discord-mdn-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -r dotenv/config -x ts-node --files ./src/index.ts",
    "start": "node build/index.js",
    "build": "yarn test:ci && tsc",
    "lint": "eslint src && tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "lint:fix": "eslint --fix src && prettier --write src",
    "lint:types": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mdn/browser-compat-data": "^3.3.0",
    "@sentry/node": "6.2.5",
    "compare-versions": "3.6.0",
    "date-fns": "2.21.1",
    "discord.js": "12.5.3",
    "dom-parser": "0.1.6",
    "html-entities": "1.4.0",
    "mongoose": "5.12.4",
    "node-cache": "5.1.2",
    "node-fetch": "2.6.1",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "devDependencies": {
    "@sentry/types": "6.2.5",
    "@types/dom-parser": "0.1.1",
    "@types/jest": "26.0.22",
    "@types/mongoose": "5.10.5",
    "@types/node": "14.14.41",
    "@types/node-fetch": "2.5.10",
    "dotenv": "8.2.0",
    "eslint": "7.24.0",
    "eslint-config-galex": "0.9.7",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "ts-jest": "26.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write",
    "*.js": "eslint --fix"
  }
}
