{
  "name": "discord-mdn-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "TS_NODE_FILES=true TS_NODE_PROJECT=\"./tsconfig.json\" nodemon -r dotenv/config -x  node --experimental-specifier-resolution=node --loader ts-node/esm ./src/index.ts",
    "start": "node build/index.js",
    "build": "yarn test:ci && tsc",
    "lint": "eslint src && tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "lint:fix": "eslint --fix src && prettier --write src",
    "lint:types": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mdn/browser-compat-data": "^4.0.6",
    "@sentry/node": "6.13.3",
    "compare-versions": "3.6.0",
    "date-fns": "2.25.0",
    "discord.js": "^13.2.0",
    "dom-parser": "0.1.6",
    "domyno": "^1.0.1",
    "html-entities": "2.3.2",
    "lodash": "^4.17.21",
    "mongoose": "6.0.11",
    "node-cache": "5.1.2",
    "node-fetch": "3.0.0",
    "ts-node": "^10.3.0",
    "typescript": "4.4.4"
  },
  "devDependencies": {
    "@sentry/types": "6.13.3",
    "@types/dom-parser": "^0.1.1",
    "@types/html-entities": "^1.3.3",
    "@types/jest": "27.0.2",
    "@types/mongoose": "5.11.96",
    "@types/node": "16.11.0",
    "@types/node-fetch": "3.0.2",
    "dotenv": "10.0.0",
    "eslint": "8.0.1",
    "eslint-config-galex": "3.0.1",
    "husky": "7.0.2",
    "jest": "27.2.5",
    "lint-staged": "11.2.3",
    "nodemon": "2.0.13",
    "prettier": "2.4.1",
    "ts-jest": "27.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write",
    "*.js": "eslint --fix"
  }
}
